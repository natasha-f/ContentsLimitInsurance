
@model Insurance.Web.Models.ContentsViewModel
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewData["Title"] = "Contents Limit Insurance";
}

@Html.React("Contents", new {
    model = Model,
    addItemUrl = Url.Action("AddItem"),
    deleteItemUrl = Url.Action("DeleteItem")
})


<script crossorigin src="https://cdnjs.cloudflare.com/ajax/libs/react/16.13.0/umd/react.development.js"></script>
<script crossorigin src="https://cdnjs.cloudflare.com/ajax/libs/react-dom/16.13.0/umd/react-dom.development.js"></script>
    
<script src="@Url.Content("~/js/index.jsx")"></script>
@Html.ReactInitJavaScript()
